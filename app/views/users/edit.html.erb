<h1>Users#edit</h1>
<p>Find me in app/views/users/edit.html.erb</p>
<%= form_for @user, url: edit_user_path do |f| %>
<%= f.text_field :username, placeholder: "Enter your username" %>
<%= f.email_field :email, placeholder: "Enter your email" %>
<%= f.password_field :password, placeholder: "Enter your password" %>
<%= f.submit "Update" %>
<% end %>
